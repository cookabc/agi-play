{
  "name": "pose",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel ./src/index.html --open",
    "clean": "rimraf ../../public/pose && rimraf .parcel-cache dist",
    "build": "parcel build --no-scope-hoist --no-source-maps ./src/index.html --public-url /pose",
    "deploy": "npm run clean && npm run build && cp -r dist/ ../../public/pose/"
  },
  "devDependencies": {
    "@parcel/transformer-less": "^2.10.0",
    "buffer": "^5.5.0||^6.0.0",
    "parcel": "^2.10.0",
    "postcss": "^8.4.31",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "util": "^0.12.3"
  },
  "dependencies": {
    "@tensorflow-models/facemesh": "^0.0.1",
    "@tensorflow-models/posenet": "^2.2.1",
    "@tensorflow/tfjs": "^1.7.0",
    "@tensorflow/tfjs-converter": "^1.7.0",
    "@tensorflow/tfjs-core": "^1.7.0",
    "babel-polyfill": "~6.26.0",
    "dat.gui": "^0.7.2",
    "face-api.js": "^0.22.1",
    "lu2": "^2023.9.15",
    "paper": "^0.12.17",
    "stats.js": "^0.17.0"
  },
  "browserslist": [
    "> 0.3%",
    "last 2 versions",
    "not dead"
  ]
}
