{
  "name": "pacman",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel ./src/index.html --open",
    "clean": "rimraf ../../public/pacman && rimraf .parcel-cache dist",
    "build": "parcel build --no-scope-hoist --no-source-maps ./src/index.html --public-url /pacman",
    "deploy": "npm run clean && npm run build && cp -r dist/ ../../public/pacman/"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.10.0",
    "@parcel/transformer-less": "^2.10.0",
    "buffer": "^5.5.0||^6.0.0",
    "parcel": "^2.10.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss": "^8.4.31",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "util": "^0.12.3"
  },
  "dependencies": {
    "@teachablemachine/image": "^0.8.5"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "ios >= 10",
    "android >= 6"
  ]
}
