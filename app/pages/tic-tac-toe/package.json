{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "scripts": {
    "dev": "parcel ./src/index.html --open",
    "clean": "rimraf ../../public/tic-tac-toe && rimraf .parcel-cache dist",
    "build": "parcel build --no-scope-hoist --no-source-maps ./src/index.html --public-url /tic-tac-toe",
    "deploy": "npm run clean && npm run build && cp -r dist/ ../../public/tic-tac-toe/"
  },
  "devDependencies": {
    "@parcel/transformer-less": "^2.10.0",
    "buffer": "^5.5.0||^6.0.0",
    "parcel": "^2.10.0",
    "postcss": "^8.4.31",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "util": "^0.12.3"
  },
  "dependencies": {
    "@tensorflow/tfjs": "1.7.4",
    "react": "16.4.2",
    "react-dom": "16.4.2"
  },
  "browserslist": [
    "> 0.3%",
    "last 2 versions",
    "not dead"
  ]
}
